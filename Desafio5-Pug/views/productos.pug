extends inicio 
block productos 

h2(style="color:crimson;")="Lista de Productos"


    div(class="table-responsive")
        table( class="table table-dark")
            tr(style="color: yellow;") 
                th="Producto"
                th="Precio"
                th="Imagen"
            
                td=producto.nombre
                td=producto.precio
                td=producto.url

    h3(class="alert alert-danger">)="No hay Productos"
